{
  "id": "open-ai",
  "name": "OpenAI",
  "description": "OpenAI integration for Mattermost",
  "icon_path": "assets/open_ai.svg",
  "version": "1.1.0",
  "min_server_version": "7.8.2",
  "server": {
    "executables": {
      "linux-amd64": "server/dist/plugin-linux-amd64",
      "linux-arm64": "server/dist/plugin-linux-arm64",
      "darwin-amd64": "server/dist/plugin-darwin-amd64",
      "darwin-arm64": "server/dist/plugin-darwin-arm64",
      "windows-amd64": "server/dist/plugin-windows-amd64.exe"
    }
  },
  "webapp": {
    "bundle_path": "webapp/dist/main.js"
  },
  "settings_schema": {
    "header": "",
    "footer": "",
    "settings": [
      {
        "key": "openAiApiBaseUrl",
        "display_name": "OpenAI API base URL",
        "type": "text",
        "help_text": "Enter the base URL for OpenAI API",
        "placeholder": "",
        "default": null
      },
      {
        "key": "openAiApiKey",
        "display_name": "OpenAI API key",
        "type": "text",
        "help_text": "Enter the OpenAI API key",
        "default": null
      },
      {
        "key": "openAiOrganizationId",
        "display_name": "OpenAI organization ID",
        "type": "text",
        "help_text": "Enter the OpenAI organization ID",
        "default": null
      }
    ]
  }
}
